		<script src="/client/script/directives/empty-links.js"></script>
		<script src="/client/script/directives/sidebars.js"></script>
		<script src="/client/script/directives/off-click.js"></script>
		<script src="/client/script/directives/full-height.js"></script>
		<script src="/client/script/directives/panel-tools.js"></script>
		<script src="/client/script/directives/char-limit.js"></script>
		<script src="/client/script/directives/dismiss.js"></script>
		<script src="/client/script/directives/compare-to.js"></script>
		<script src="/client/script/directives/select.js"></script>
		<script src="/client/script/directives/messages.js"></script>
		<script src="/client/script/directives/chat.js"></script>
		<script src="/client/script/directives/sparkline.js"></script>
		<script src="/client/script/directives/touchspin.js"></script>
		<script src="/client/script/directives/file-upload.js"></script>
		<!-- Clip-Two Controllers -->
		<script src="/client/script/controllers/mainCtrl.js"></script>
