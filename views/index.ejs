		<script src="/vendor/oclazyload/dist/ocLazyLoad.min.js"></script>
		<!-- breadcrumb -->
		<script src="/vendor/angular-breadcrumb/dist/angular-breadcrumb.min.js"></script>
		<!-- UI Bootstrap -->
		<script src="/vendor/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
		<!-- Loading Bar -->
		<script src="/vendor/angular-loading-bar/build/loading-bar.min.js"></script>
		<!-- Angular Scroll -->
		<script src="/vendor/angular-scroll/angular-scroll.min.js"></script>
		<!-- Clip-Two Scripts -->
		<script src="/client/script/app.js"></script>
		<script src="/client/script/main.js"></script>
		<script src="/client/script/config.constant.js"></script>
		<script src="/client/script/config.router.js"></script>
